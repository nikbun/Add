@model IEnumerable<AddressBook.Models.Address>

<!--Таблица-->
<table class="table  table-striped">
    <thead class="thead-dark">
        <tr>
            <th scope="col">@Html.DisplayNameFor(model => model.Country)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.City)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Street)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.HouseNumber)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Date)</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count(); i++)
        {
            <tr>
                <td>@Model.ElementAt(i).Country</td>
                <td>@Model.ElementAt(i).City</td>
                <td>@Model.ElementAt(i).Street</td>
                <td>@Model.ElementAt(i).HouseNumber</td>
                <td>@Html.DisplayFor(model => model.ElementAt(i).Date)</td>
            </tr>
        }
    </tbody>
</table>