@model IEnumerable<AddressBook.Models.Address>

<!--Таблица-->
<table class="table table-hover">
    <thead class="thead-dark fixed-head">
        <tr>
            <th scope="col">@Html.DisplayNameFor(model => model.Country)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.City)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Street)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.HouseNumber)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.TypeBuilding.TypeBuild)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Date)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (AddressBook.Models.Address address in Model)
        {
        <tr class="edit-address">
            <td style="display:none">@address.AddressId</td>
            <td>@address.Country</td>
            <td>@address.City</td>
            <td>@address.Street</td>
            <td>@address.HouseNumber</td>
            <td style="display:none">@address.TypeBuildId</td>
            <td>@address.TypeBuilding.TypeBuild</td>
            <td>@Html.DisplayFor(model => address.Date)</td>
            <!--<td class="edit"><img  src="~/Images/pencil.png"></td>-->
        </tr>
        }
    </tbody>
</table>

@Scripts.Render("~/bundles/TableScripts")